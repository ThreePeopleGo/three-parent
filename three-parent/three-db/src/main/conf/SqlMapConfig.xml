<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
"http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
 <!-- 配置settings -->  
  <settings lazyLoadingEnabled="true" useStatementNamespaces="true"/>   
<!-- 这里可以写多个实体的映射文件 -->
<!-- 权限 -->
<sqlMap resource="com/three/caideli/mapping/BackPowerMapper.xml" />
<!-- 用户 -->
<sqlMap resource="com/three/caideli/mapping/BackUserMapper.xml" />
<!-- 角色 -->
<sqlMap resource="com/three/caideli/mapping/BackRoleMapper.xml" />
<!-- 角色权限 -->
<sqlMap resource="com/three/caideli/mapping/BackRolePowerMapper.xml" />
<!--用户角色  -->
<sqlMap resource="com/three/caideli/mapping/BackUserRoleMapper.xml" />
</sqlMapConfig>